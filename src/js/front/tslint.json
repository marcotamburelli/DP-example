{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "member-ordering": [
      true,
      {
        "order": "statics-first"
      }
    ],
    "no-unused-variable": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "no-any": false,
    "member-access": false,
    "indent": [
      true,
      "spaces"
    ],
    "no-var-keyword": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "jsdoc-format": true,
    "whitespace": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "switch-default": false,
    "only-arrow-functions": [
      false
    ],
    "radix": false,
    "no-namespace": false,
    "arrow-parens": false,
    "triple-equals": false,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "no-empty": false,
    "no-unused-expression": false,
    "array-type": [
      true,
      "array"
    ],
    "space-before-function-paren": false,
    "no-submodule-imports": [
      true,
      "rxjs"
    ]
  }
}