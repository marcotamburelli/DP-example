{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "builtin": {
    "WeakMap": false
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "eqeqeq": "warn",
    "curly": "warn",
    "valid-jsdoc": "error",
    "camelcase": "error",
    "no-console": "warn",
    "no-trailing-spaces": "warn",
    "newline-after-var": [
      "error",
      "always"
    ],
    "newline-before-return": "error",
    "eol-last": "error",
    "valid-typeof": "error",
    "no-empty": "warn",
    "no-unused-vars": "warn"
  }
}